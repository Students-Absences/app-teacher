{
    "extends": "@tsconfig/react-native/tsconfig.json",
    "allowJs": false,
    "compilerOptions": {
        "baseUrl": ".",
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "paths": {
            "@/*": ["./src/*"],
            "@components/*": ["./src/components/*"]
        }
    },
    "forceConsistentCasingInFileNames": true
}
